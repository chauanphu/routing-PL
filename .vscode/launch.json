{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug vrppl_loader",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/yu-sa/build/vrppl_loader",
            "args": ["sa", "small"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb"
        },
